EXCLAMATION             '!'

# True
DOUBLEEXCLAMATION       '!!'
# Fasle
TRIPLEEXCLAMATION       '!!!'
# and
DOUBLEAMPERS            '&&'
# or
DOUBLEVBAR              '||'
# not
DOUBLETILDE             '~~'
# if
QUESTION                '?'
# elif
QUESTIONQUESTION        '?!?'
# else
QUESTIONEXCLAMATION     '?!'
# assert_if
CIRCUMFLEXQUESTION      '^?'
# assert
CIRCUMFLEXQUESTIONEXCLAMATION  '^?!'
# dummy raise           
DOUBLECIRCUMFLEX        '^^'
# raise                 
TRIPLECIRCUMFLEX        '^^^'
# try
TILDECIRCUMFLEX         '~^'
# except
QUESTIONCIRCUMFLEX      '?^'
# try_else
QUESTIONEXCLAMATIONCIRCUMFLEX  '?!^'
# finally
QUESTIONASTERISK        '?*'
# as
EQUALRARROW             '=>'


