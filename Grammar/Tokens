EXCLAMATION             '!'

# and
DOUBLEAMPERS            '&&'
# or
DOUBLEVBAR              '||'
# if
QUESTION                '?'
# else / elsif
QUESTIONQUESTION        '??'
# assert_if
CIRCUMFLEXQUESTION      '^?'
# assert
CIRCUMFLEXQUESTIONEXCLAMATION  '^?!'
# dummy raise           
DOUBLECIRCUMFLEX        '^^'
# raise                 
TRIPLECIRCUMFLEX        '^^^'
# try
TILDECIRCUMFLEX         '~^'
# as
EQUALRARROW             '=>'
# for
DOLLAR                  '$'
# while
DOLLARQUESTION          '$?'
# dummy break
DOLLARGREATER          '$>'
# break
DOLLARGREATERGREATER   '$>>'
# dummy continue
GREATERDOLLAR           '$<'
# continue
GREATERGREATERDOLLAR   '$<<'
# is
TRIPLEEQUAL            '==='
# true
DOUBLEPLUS             '++'
# false
DOUBLEMINUS            '--'
# return alt
LARROW                 '<-'
# yield
TILDEGREATER           '~>'
# yield alt
LESSTILDE              '<~'
# from
DOTGREATER             '.>'
# import
LESSDOT                '<.'
# dummy match
QUESTIONEQUAL          '?='
# match
QUESTIONDOUBLEEQUAL    '?=='
# pipe
VBARGREATER             '|>'
# is_not
NOTEQUALEQUAL           '!=='
# del
LESSGREATER             '<>'

